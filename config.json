{
  "devices": [
    {
      "serialPath": "/dev/ttyUSB0",
      "deviceId": "bluetooth_device_0",
      "baudRate": 115200,
      "enabled": true
    },
    {
      "serialPath": "/dev/ttyUSB1",
      "deviceId": "bluetooth_device_1",
      "baudRate": 115200,
      "enabled": false
    }
  ],
  "rssi": "-60",
  "useConfigRssi": true,
  "reportInterval": 6000,
  "serialTransport": {
    "serialPath": "/dev/ttyS3",
    "baudRate": 115200,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "none",
    "timeout": 5000
  },
  "logging": {
    "level": "info",
    "enableDevicePrefix": true,
    "enableTimestamp": true
  }
}
